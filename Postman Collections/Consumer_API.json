          "script": {
            "type": "text/javascript",
            "exec": [
              "tests[\"Status code is 200\"] = responseCode.code === 200;",
              "",
              "var response = JSON.parse(responseBody);",
              "var Tests = [];",
              "",
              "for (var i in response.tests){",
              "    var obj = response.tests[i];",
              "    ",
              "    if(obj.test_name === \"dummyTest\") {",
              "        tests[obj.test_name + \" Passed\"] = obj.test_result === \"passed\";",
              "    }else{",
              "        tests[obj.test_name + \" Failed\"] = obj.test_result === \"failed\";",
              "    }",
              "}"
            ]
